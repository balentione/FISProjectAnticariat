<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ebab7f;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.LoggedInController">
   <children>
      <Label fx:id="label_hello" layoutX="253.0" layoutY="26.0" prefHeight="59.0" prefWidth="94.0" text="Hello!  " textAlignment="CENTER">
         <font>
            <Font name="Bangla MN" size="25.0" />
         </font>
      </Label>
      <Label fx:id="label_welcome" layoutX="113.0" layoutY="85.0" text="Welcome to bookshop!">
         <font>
            <Font name="Bangla MN" size="30.0" />
         </font>
      </Label>
      <ImageView fitHeight="200.0" fitWidth="600.0" layoutY="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/books.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="button_logout" layoutX="246.0" layoutY="146.0" mnemonicParsing="false" onAction="#handleLogout" prefHeight="26.0" prefWidth="108.0" style="-fx-background-color: white; -fx-cursor: hand;" text="Logout">
         <font>
            <Font name="Bangla MN" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
